{
  "name": "plotly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts --max_old_space_size=8192 start",
    "build": "react-scripts --max_old_space_size=8192 build",
    "test": "react-scripts test",
    "test:once": "react-scripts test --no-watch",
    "eject": "react-scripts eject",
    "netlify:deploy:auto": "netlify deploy --dir=./build -p --site=$NETLIFY_AUTOENV_SITE_ID --auth=$NETLIFY_AUTH_TOKEN",
    "netlify:deploy:test": "netlify deploy --dir=./build -p --site=$NETLIFY_TESTENV_SITE_ID --auth=$NETLIFY_AUTH_TOKEN",
    "netlify:deploy:staging": "netlify deploy --dir=./build -p --site=$NETLIFY_STAGINGENV_SITE_ID --auth=$NETLIFY_AUTH_TOKEN"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "lodash": "^4.17.11",
    "plotly.js": "^1.46.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-plotly.js": "^2.3.0",
    "react-router-dom": "^5.0.0",
    "react-s-alert": "^1.4.1",
    "react-scripts": "2.1.8",
    "reactstrap": "^8.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "history": "^4.9.0",
    "husky": "^1.3.1",
    "jest-dom": "^3.1.3",
    "jest-fetch-mock": "^2.1.2",
    "netlify-cli": "^2.11.7",
    "react-testing-library": "^6.1.2"
  }
}
